package deptycheck

authors = "Denis Buzdalov"

brief = "Property-based testing with dependent types"

license = "MPL-2.0"

sourcedir = "src"
builddir = ".build"

version = 0.0.250319

modules = Deriving.DepTyCheck
        , Deriving.DepTyCheck.Gen
        , Deriving.DepTyCheck.Gen.ConsRecs
        , Deriving.DepTyCheck.Gen.ForAllNeededTypes.Impl
        , Deriving.DepTyCheck.Gen.ForAllNeededTypes.Interface
        , Deriving.DepTyCheck.Gen.ForOneType.Impl
        , Deriving.DepTyCheck.Gen.ForOneType.Interface
        , Deriving.DepTyCheck.Gen.ForOneTypeCon.Impl
        , Deriving.DepTyCheck.Gen.ForOneTypeConRhs.Impl
